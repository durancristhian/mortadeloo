{
  "name": "mortadeloo",
  "version": "0.1.0",
  "description": "mortadeloo app",
  "scripts": {
    "prebuild": "de public/**/*.*",
    "build": "npm run build-css & npm run build-images & npm run build-js",
    "build-css": "copyfiles -u 1 client/css/**/*.css public",
    "build-images": "copyfiles -u 1 client/img/*.* public",
    "get-dreams": "babel-node server/scripts/dreams",
    "prebuild-js": "copyfiles -u 1 client/js/app.js public",
    "build-js": "browserify -d -t [ babelify --presets [ es2015 ] ] client/js/app.js | exorcist public/js/app.map.js > public/js/app.js",
    "lint": "eslint **/*.js",
    "lint-html": "eslint --format html **/*.js > eslint.html",
    "postinstall": "npm run build",
    "start": "babel-node server/index",
    "start-dev": "nodemon --watch server/**/*.js --exec npm run start"
  },
  "author": "Duran Cristhian (@durancristhian)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.4.5",
    "babel-eslint": "5.0.0-beta6",
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "cheerio": "0.19.0",
    "compression": "1.6.1",
    "cookie-parser": "1.4.1",
    "copyfiles": "0.2.1",
    "del-cli": "0.1.2",
    "dotenv": "2.0.0",
    "eslint": "1.10.3",
    "exorcist": "0.4.0",
    "express": "4.13.4",
    "express-handlebars": "2.0.1",
    "express-session": "1.13.0",
    "ghooks": "1.0.3",
    "helmet": "1.1.0",
    "jquery": "2.2.0",
    "moment": "2.11.1",
    "mongoose": "4.3.7",
    "mongoose-findorcreate": "0.1.2",
    "nodemon": "1.8.1",
    "passport": "0.3.2",
    "passport-twitter": "1.0.3",
    "request": "2.67.0",
    "simple-node-logger": "0.92.21"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "escope": "3.3.0"
  }
}
